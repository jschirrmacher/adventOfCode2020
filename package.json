{
  "name": "adventofcode",
  "version": "1.0.0",
  "engines": {
    "node": ">= 14.0.0 <15.0.0",
    "npm": ">= 6.0.0"
  },
  "description": "My solution for Advent of code 2020",
  "scripts": {
    "start": "ts-node server.ts",
    "start-dev": "ts-node-dev server.ts --watch",
    "lint": "eslint .",
    "test": "mocha -r ts-node/register puzzles/*.spec.ts",
    "build": "tsc && docker build -t aoc2020 .",
    "in-docker": "docker run -d --rm --name aoc2020 -p 3000:3000 aoc2020",
    "docker-stop": "docker stop aoc2020"
  },
  "author": "joachimschirrmacher@gmail.com",
  "license": "Apache 2.0",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-json-format": "^2.0.1",
    "mocha": "^8.2.1",
    "should": "^13.2.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
}
